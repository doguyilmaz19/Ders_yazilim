Private Sub Button2_Click(sender As Object, e As EventArgs) Handles Button2.Click
        Dim dersad As String = InputBox("Ders Adınızı giriniz")
        CheckedListBox1.Items.Add(dersad)
    End Sub

    Private Sub Button3_Click(sender As Object, e As EventArgs) Handles Button3.Click

        Dim i As Integer = 0
        Dim s As Integer = 0
        If CheckedListBox1.Items.Count = 0 Then
            MsgBox("Kayıtlı dersiniz yoktur...")
        End If
        For i = 0 To CheckedListBox1.CheckedItems.Count - 1

            ListBox2.Items.Add(CheckedListBox1.CheckedItems(i).ToString())
        Next

        For s = 0 To ListBox2.Items.Count - 1
            CheckedListBox1.Items.Remove(ListBox2.Items(s))
        Next
    End Sub

    Private Sub Button11_Click(sender As Object, e As EventArgs) Handles Button11.Click
        If ListBox2.Items.Count = 0 Then
            MsgBox("Silinecek ders yok")
        End If
        ListBox2.Items.Clear()
    End Sub


    Public basarinot, ders As String
    Private Sub Button4_Click(sender As Object, e As EventArgs) Handles Button4.Click
        Dim j As Integer = 0
        Dim v, v2, f As Integer
        Dim bn As Double

        If CheckedListBox1.CheckedItems.Count > 1 Then
            MsgBox("Lütfen birden fazla ders seçmeyiniz ve mavi seçim yeri istediğiniz dersin üzerinde olsun")
        Else
            v = InputBox(CheckedListBox1.SelectedItem & " Dersi Vize-1 Notunuzu Giriniz")
            v2 = InputBox(CheckedListBox1.SelectedItem & " Dersi Vize-2 Notunuzu Giriniz")
            f = InputBox(CheckedListBox1.SelectedItem & " Dersi Final Notunuzu Giriniz")
            bn = (v * 0.2) + (v2 * 0.2) + (f * 0.6)
            MsgBox("Başarı notunuz" & bn)
            basarinot = bn.ToString()
            ders = CheckedListBox1.SelectedItem
            MsgBox("Lütfen Yazdır butonuna basınız")
        End If
    End Sub

    Private Sub Button9_Click(sender As Object, e As EventArgs) Handles Button9.Click
        Me.Close()
    End Sub
    Private Sub Button5_Click(sender As Object, e As EventArgs) Handles Button5.Click
        CheckedListBox1.Items.Clear()
        ListBox2.Items.Clear()
    End Sub
    Public say As Integer
    Private Sub Button10_Click_1(sender As Object, e As EventArgs) Handles Button10.Click
        OpenFileDialog1.ShowDialog()
        PictureBox1.ImageLocation = OpenFileDialog1.FileName
    End Sub
    Private Sub Button6_Click(sender As Object, e As EventArgs) Handles Button6.Click
        TextBox1.Text = String.Empty
        TextBox2.Text = String.Empty
        TextBox3.Text = String.Empty
        TextBox4.Text = String.Empty
        TextBox6.Text = String.Empty
        RadioButton1.Checked = False
        RadioButton2.Checked = False
        RadioButton3.Checked = False
        RadioButton4.Checked = False
        RadioButton5.Checked = False

        CheckBox1.Checked = False
        CheckBox2.Checked = False
        CheckBox3.Checked = False
        CheckBox4.Checked = False
        CheckBox5.Checked = False
        CheckBox6.Checked = False
        CheckBox7.Checked = False

        CheckedListBox1.Items.Clear()
        ListBox2.Items.Clear()
    End Sub
    Private Sub Button7_Click(sender As Object, e As EventArgs) Handles Button7.Click
        Dim tc, ad, soyad, adres, cinsiyet, uyruk As String
        tc = TextBox1.Text
        ad = TextBox2.Text
        soyad = TextBox3.Text
        adres = TextBox4.Text

        If RadioButton1.Checked = True Then
            cinsiyet = RadioButton1.Text
        ElseIf RadioButton2.Checked = True Then
            cinsiyet = RadioButton2.Text
        Else
            cinsiyet = RadioButton5.Text
        End If

        If RadioButton4.Checked = True Then
            uyruk = RadioButton4.Text
        ElseIf RadioButton3.Checked = True Then
            uyruk = RadioButton3.Text
        End If

        Dim oge As New ListViewItem(tc)
        oge.SubItems.Add(ad)
        oge.SubItems.Add(soyad)
        oge.SubItems.Add(adres)
        oge.SubItems.Add(cinsiyet)
        oge.SubItems.Add(uyruk)
        oge.SubItems.Add(okul)
        oge.SubItems.Add(ders)
        oge.SubItems.Add(basarinot)
        oge.ImageIndex = say - 1
        ListView1.Items.Add(oge)

        'işlemleri sayıyorum'
        say = say + 1

        If say = ogrs Then
            GroupBox1.Enabled = False
            GroupBox2.Enabled = False
            GroupBox3.Enabled = False
            GroupBox4.Enabled = False
            MsgBox("Öğrenci Sayınızı Doldurdunuz daha fazla öğrenci girmek istiyorsanız tekrar işlem yapacağınız öğrenci sayısını belirtiniz")
            TextBox5.Text = String.Empty
            GroupBox5.Enabled = True

        End If
    End Sub
End Class

	
FORM 2(ADRESFORM)
Public Class f_adresgir
    Private Sub f_adresgir_Load(sender As Object, e As EventArgs) Handles MyBase.Load

    End Sub

    Private Sub ComboBox1_SelectedIndexChanged(sender As Object, e As EventArgs) Handles ComboBox1.SelectedIndexChanged
        'ilçeler'
        Select Case ComboBox1.SelectedIndex

            Case 0
                ComboBox2.Items.Clear()
                ComboBox2.Text = String.Empty
                ComboBox2.Items.Add("Buca")
                
            Case 1
                ComboBox2.Items.Clear()
                ComboBox2.Text = String.Empty ComboBox2.Items.Add("Şişli")ComboBox2.Items.Add("Kadıköy") ComboBox2.Items.Add("Avcılar")             
            Case 2
                ComboBox2.Items.Clear()
                ComboBox2.Text = String.Empty
ComboBox2.Items.Add("Keçiör")ComboBox2.Items.Add("Çubuk")ComboBox2.Items.Add("Elmadağ")      
        End Select
    End Sub
Private Sub ComboBox2_SelectedIndexChanged(sender As Object, e As EventArgs) Handles ComboBox2.SelectedIndexChanged
        'mahalleler'
        If ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 0 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Adatepe")
            ComboBox3.Items.Add("Atatürk")
            ComboBox3.Items.Add("Belenbaşı")
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 1 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Bahçelerarası")
            ComboBox3.Items.Add("Çetin")
            ComboBox3.Items.Add("Eğitim")
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 2 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Alaybey")
            ComboBox3.Items.Add("Bahçelievler")
            ComboBox3.Items.Add("Bostanlı")
        End If
        If ComboBox1.SelectedIndex = 1 And ComboBox2.SelectedIndex = 0 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Bozkurt")
            ComboBox3.Items.Add("Cumhuriyet")
            ComboBox3.Items.Add("Eskişehir")
        ElseIf ComboBox1.SelectedIndex = 1 And ComboBox2.SelectedIndex = 1 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Acıbadem")
            ComboBox3.Items.Add("Bostancı")
            ComboBox3.Items.Add("Erenköy")
        ElseIf ComboBox1.SelectedIndex = 1 And ComboBox2.SelectedIndex = 2 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Ambarlı")
            ComboBox3.Items.Add("Tahtakale")
            ComboBox3.Items.Add("Merkez")
        End If
        If ComboBox1.SelectedIndex = 2 And ComboBox2.SelectedIndex = 0 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Adnan Menderes")
            ComboBox3.Items.Add("Aktepe")
            ComboBox3.Items.Add("Bademlik")

        ElseIf ComboBox1.SelectedIndex = 2 And ComboBox2.SelectedIndex = 1 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Ağılcık")
            ComboBox3.Items.Add("Esemboğa")
            ComboBox3.Items.Add("Kızılca")
        ElseIf ComboBox1.SelectedIndex = 2 And ComboBox2.SelectedIndex = 2 Then
            ComboBox3.Items.Clear()
            ComboBox3.Text = String.Empty
            ComboBox3.Items.Add("Akçaali")
            ComboBox3.Items.Add("Gümüşpala")
            ComboBox3.Items.Add("Deliler")
        End If


    End Sub
Private Sub ComboBox3_SelectedIndexChanged(sender As Object, e As EventArgs) Handles ComboBox3.SelectedIndexChanged
        'caddeler'
        'İzmir'
        'Buca İlçesinin'
        'Adatepe Mahallesinin Caddeleri'
        If ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 0 And ComboBox3.SelectedIndex = 0 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35400")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Adatepe Caddesi ")
            ComboBox4.Items.Add("Çoban Günerli Caddesi 2")
            ComboBox4.Items.Add("Münasip Caddesi")
            'Atatürk Mahallesi Caddeleri'
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 0 And ComboBox3.SelectedIndex = 1 Then

            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35390")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Kayık Ali Caddesi")
            ComboBox4.Items.Add("Hasan Tahsin Caddesi")
            ComboBox4.Items.Add("İrfan Doğmuş Caddesi")
            'Belenbaşı Mahallesi Caddeleri'
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 0 And ComboBox3.SelectedIndex = 2 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35390")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Saadet Güneş Caddesi ")
            ComboBox4.Items.Add("Mustafa Ateş Caddesi ")
            ComboBox4.Items.Add("İhsan Köse Caddesi ")
        End If
  'Balçova İlçesinin'
        'Bahçelerarası Mahallesi Caddeleri'
        If ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 1 And ComboBox3.SelectedIndex = 0 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35330")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Şerif Müneaaak")
            ComboBox4.Items.Add("İlhan Yılmaz Caddesi")
            ComboBox4.Items.Add("Sedat Pekar Caddesi")
            'Çetin Mahallesi Caddeleri'
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 1 And ComboBox3.SelectedIndex = 1 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35390")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Mustafa Caddesi")
            ComboBox4.Items.Add("Kenan Caddesi")
            ComboBox4.Items.Add("Mehmet İhsan Tımarlı Caddesi")
            'Eğitim Mahallesi Caddeleri'
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 1 And ComboBox3.SelectedIndex = 2 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35390")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Belenbaşı Caddesi")
            ComboBox4.Items.Add("Belenbaşı Caddesi 1")
            ComboBox4.Items.Add("Belenbaşı Caddesi 2")
        End If
        'Karşıyaka İlçesinin'
        'Alaybey Mahallesi Caddeleri'
        If ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 2 And ComboBox3.SelectedIndex = 0 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
            ComboBox6.Items.Clear()
            ComboBox6.Items.Add("35600")
            ComboBox4.Text = String.Empty
            ComboBox4.Items.Add("Sinan Kin")
            ComboBox4.Items.Add("Ürküp  Caddesi")
            ComboBox4.Items.Add("Ramzi Hasan Caddesi")
            'Bahçelievler Mahallesi Caddeleri'
        ElseIf ComboBox1.SelectedIndex = 0 And ComboBox2.SelectedIndex = 2 And ComboBox3.SelectedIndex = 1 Then
            ComboBox4.Items.Clear()
            'combobox 5 e mahallenin postakodunu yazıyorum'
