    Public tamadres, postakodu As String

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        If ComboBox1.Text = String.Empty Or ComboBox2.Text = String.Empty Or ComboBox3.Text = String.Empty Or ComboBox4.Text = String.Empty Or ComboBox5.Text = String.Empty Or ComboBox6.Text = String.Empty Or TextBox6.Text = String.Empty Or TextBox7.Text = String.Empty Then
            MsgBox("Lütfen bütün alanları doldurunuz")
        Else
            tamadres = ComboBox5.Text & " " & ComboBox4.Text & " " & ComboBox3.Text & " " & ComboBox2.Text & " " & ComboBox1.Text
            postakodu = ComboBox6.Text
            Form1.TextBox4.Text = tamadres
            Form1.TextBox6.Text = postakodu
            Form1.Show()
            Me.Hide()
        End If
    End Sub
End Class

